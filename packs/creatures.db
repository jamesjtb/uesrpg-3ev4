o Fire 3 (Weakness 3, 6 MP)</li>\n</ul>\n</li>\n</ul>","traits":"","powers":"","spells":"","combat_styles":{"cs1":{"name":"","rank":0,"bonus":0,"weapons":""},"cs2":{"name":"","rank":0,"bonus":0,"weapons":""},"cs3":{"name":"","rank":0,"bonus":0,"weapons":""}},"weapons":{"w1":{"name":"","dmg":"","hands":"","reach":"","qualities":""},"w2":{"name":"","dmg":"","hands":"","reach":"","qualities":""},"w3":{"name":"","dmg":"","hands":"","reach":"","qualities":""},"w4":{"name":"","dmg":"","hands":"","reach":"","qualities":""},"w5":{"name":"","dmg":"","hands":"","reach":"","qualities":""}},"shield":{"name":"","br":null,"magic_br":"","class":"","qualities":""},"armor_class":"none","armor_class_cat":{"none":"None","super_light":"Super Light","light":"Light","medium":"Medium","heavy":"Heavy","super_heavy":"Super Heavy"},"armor":{"head":{"name":"","enc":0,"ar":null,"magic_ar":"","class":""},"body":{"name":"","enc":0,"ar":null,"magic_ar":"","class":""},"r_leg":{"name":"","enc":0,"ar":null,"magic_ar":"","class":""},"l_leg":{"name":"","enc":0,"ar":null,"magic_ar":"","class":""},"r_arm":{"name":"","enc":0,"ar":null,"magic_ar":"","class":""},"l_arm":{"name":"","enc":0,"ar":null,"magic_ar":"","class":""}},"professions":{"combat":60,"magic":70,"evade":40,"observe":35,"stealth":20,"knowledge":35,"social":5,"physical":50}},"sort":100001,"flags":{},"img":"systems/uesrpg-d100/creature-compendium/flame_atronach.webp","token":{"flags":{"token-auras":{"aura1":{"colour":"#ffffff","opacity":0.5,"distance":null,"square":false},"aura2":{"colour":"#ffffff","opacity":0.5,"distance":null,"square":false}}},"name":"Flame Atronach","displayName":0,"img":"systems/uesrpg-d100/creature-compendium/flame_atronach.webp","tint":"","width":1,"height":1,"scale":1,"mirrorX":false,"mirrorY":false,"lockRotation":false,"rotation":0,"vision":true,"dimSight":0,"brightSight":0,"dimLight":5,"brightLight":1,"sightAngle":360,"lightAngle":360,"lightColor":"#ff8040","lightAlpha":0.45,"lightAnimation":{"type":"torch","speed":